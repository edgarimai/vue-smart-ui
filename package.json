    "url": "git+https://github.com/edgarimai/vue-smart-ui.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "components",
    "ui",
    "frontend",
    "web-components",
    "smart-ui",
    "vue-components",
    "ui-library",
    "vue-ui",
    "customizable",
    "modern"
  ],
  "license": "MIT",
