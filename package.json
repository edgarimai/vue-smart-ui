{
  "name": "vue-smart-ui",
  "version": "0.5.1",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "description": "A collection of Vue 3 smart and highly customizable UI components for modern web applications",
  "author": "Edgar Imai",
  "homepage": "https://vuesmartui.edgarimai.com",
  "engines": {
    "node": ">=20.12.2"
  },
